{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/aria": [
      "default",
      {
        "aria-required-children": "off",
        "aria-valid-attr-value": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "position: sticky",
          "inset",
          "backdrop-filter"
        ]
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "img[loading]"
        ]
      }
    ],
    "no-inline-styles": "off"
  }
}